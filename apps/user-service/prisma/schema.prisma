generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
  output = "../node_modules/.prisma/client"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id        String   @id
  email     String   @unique
  password  String
  token     String?  @unique
  createdAt DateTime @default(now())
  updatedAt DateTime
}

model JWKS {
  id         String @id
  publicKey  String @unique
  privateKey String @unique
}
