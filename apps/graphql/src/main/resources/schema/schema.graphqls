scalar Upload

type Query {
    imageDetails(filename: String!): ImageDetails
}

type Mutation {
    createImage(name: String!, description: String!, file: Upload): ImageUpload
}

type ImageDetails {
    src: String!
    alt: String!
    width: Int!
    height: Int!
    aspectRatio: String!
    srcSet: String!
    sizes: String!
}

type ImageUpload {
    id: String!
    name: String!
    description: String!
    width: Int!
    height: Int!
    quality: Int!
    imageId: String!
    url: String!
}

